{
  "expo": {
    "name": "frontEnd",
    "slug": "frontEnd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/BodoniModa-VariableFont_opsz,wght.ttf",
            "./assets/fonts/NunitoSans-VariableFont_YTLC,opsz,wdth,wght.ttf",
            "./assets/fonts/Quicksand-VariableFont_wght.ttf",
            "./assets/fonts/RobotoSerif-VariableFont_GRAD,opsz,wdth,wght.ttf"
          ]
        }
      ]
    ]
  }
}
